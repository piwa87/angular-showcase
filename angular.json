{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "at-common-examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/at-common-examples",
      "sourceRoot": "projects/at-common-examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/at-common-examples",
            "index": "projects/at-common-examples/src/index.html",
            "browser": "projects/at-common-examples/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/at-common-examples/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/at-common-examples/src/favicon.ico",
              "projects/at-common-examples/src/assets"
            ],
            "styles": ["projects/at-common-examples/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "at-common-examples:build:production"
            },
            "development": {
              "buildTarget": "at-common-examples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "at-common-examples:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/at-common-examples/tsconfig.spec.json",
            "karmaConfig": "projects/at-common-examples/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/at-common-examples/src/favicon.ico",
              "projects/at-common-examples/src/assets"
            ],
            "styles": ["projects/at-common-examples/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "@at-common/auth": {
      "projectType": "library",
      "root": "projects/at-common/auth",
      "sourceRoot": "projects/at-common/auth/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/at-common/auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/at-common/auth/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/at-common/auth/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
